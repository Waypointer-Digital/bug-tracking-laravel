{
    "name": "waypointer-digital/bug-tracking-laravel",
    "description": "Kanbino Bug Tracking SDK for Laravel - Error capture, breadcrumbs, and session replay",
    "type": "library",
    "license": "MIT",
    "keywords": ["kanbino", "bug-tracking", "error-tracking", "laravel", "exception-handler"],
    "homepage": "https://github.com/Waypointer-Digital/bug-tracking-laravel",
    "authors": [
        {
            "name": "Waypointer Digital",
            "email": "dev@kanbino.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/queue": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Kanbino\\BugTracking\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kanbino\\BugTracking\\KanbinoServiceProvider"
            ],
            "aliases": {
                "Kanbino": "Kanbino\\BugTracking\\Facades\\Kanbino"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
